{
    "name": "opencck/cms",
    "description": "Content management system with content construction",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Krupkin Sergey",
            "email": "rekrytkw@gmail.com",
            "homepage": "https://vk.com/krupkin.sergey"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "opencck/api": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "phpunit/php-invoker": "^3.0",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "APP\\": [
                "app/",
                "app/controllers",
                "app/models",
                "app/helpers"
            ]
        }
    },
    "suggest": {
        "ext-memcached": "*",
        "ext-redis": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "APP\\": "tests/APP"
        }
    }
}
